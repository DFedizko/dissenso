---
import MinorTitle from "@/components/atoms/typography/content/MinorTitle.astro";
import LayoutContainer from "@/components/atoms/contianers/LayoutContainer.astro";
import NavLinkList from "../molecules/NavLinkList.astro";
import type { NavLink } from "../molecules/NavLinkList.astro";

const links: NavLink[] = [
    {
        link: {
            href: "/"
        },
        text: "Dissenso"
    },
    {
        link: {
            href: "/publish-article"
        },
        text: "Publicar artigo"
    },
    {
        link: {
            href: "/articles/test"
        },
        text: "Artigos"
    },
]; 
---

<header class="flex justify-center py-6">
    <LayoutContainer class="flex-row justify-between">
        <NavLinkList
            class="w-full"
            links={links}
        />
    </LayoutContainer>
</header>
